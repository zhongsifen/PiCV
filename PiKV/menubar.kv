# File name: menubar.kv
#:kivy 1.7.0
<MenuBar@BoxLayout>:
    orientation: 'horizontal'
    padding: 2
    Button:
        text: 'Start'
        #on_press: root.DrawingSpace._camera.play = True
    Button:
        text: 'Stop'
        #on_press: _camera.play = False
    Button:
        text: 'Capture'
    Button:
        text: 'Setup'
    Button:
        text: 'Exit'
